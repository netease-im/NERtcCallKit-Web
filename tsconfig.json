{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "module": "ES2015",
    "target": "es5",
    "lib": ["DOM", "ES2020"],
    "sourceMap": false,
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./node_modules/@types"]
  }
}
